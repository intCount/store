<?xml version="1.0" encoding="UTF-8"?>
<persistence xmlns="http://java.sun.com/xml/ns/persistence" version="2.0">

    <persistence-unit name="intCount">
        <class>com.intCount.Store.web.entities.Items</class>
        <properties>
            <property name="hibernate.connection.url" value="jdbc:mysql://localhost:3306/intCount"/>
            <property name="hibernate.connection.driver_class" value="com.mysql.jdbc.Driver"/>
        </properties>
    </persistence-unit>
</persistence>
